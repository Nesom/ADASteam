@model GetOrdersModel
@foreach(var order in Model.Products)
{
    <pre>Номер заказа: @order.Key.Id</pre>
    <pre>Сумма заказа: @order.Key.Amount руб</pre>
    <pre>Время заказа: @order.Key.OrderTime.ToString()</pre>
    <pre>Статус заказа: @order.Key.StatusInfo.ToString()</pre>
    <pre>Список товаров (@order.Value.Count): </pre>
    foreach(var product in order.Value)
    {
        <pre>   Название: @product.Item1.Name       Количество: @product.Item2</pre>
    }
    <br />
}
